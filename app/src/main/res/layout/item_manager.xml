<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/w240"
    android:layout_height="wrap_content"
    android:background="@drawable/sl_white_item"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/manager_icon_iv"
        android:layout_width="@dimen/w50"
        android:layout_height="@dimen/h50"
        android:layout_marginTop="@dimen/h45"
        android:layout_gravity="center_horizontal"
        android:src="@mipmap/ic_store_manager" />

    <TextView
        android:id="@+id/manager_name_tv"
        style="@style/TextBlank.Medium"
        android:layout_gravity="center_horizontal"
        android:textSize="@dimen/h28"
        android:layout_marginTop="@dimen/h25"
        android:layout_marginBottom="@dimen/h30"
        android:text="店铺管理" />
</LinearLayout>